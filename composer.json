{
  "name": "torann/remote-model",
  "description": "An eloquent-like model, for the Laravel framework.",
  "keywords": ["laravel","eloquent","model","api","remote data","custom models"],
  "license": "BSD 2-Clause",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "daniel@lyften.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "~5.1",
    "illuminate/contracts": "~5.1",
    "jenssegers/date": "^3.0",
    "illuminate/pagination": "~5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "mockery/mockery": "^0.9.4",
    "orchestra/testbench": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Torann\\RemoteModel\\": "src/Torann/RemoteModel"
    }
  },
  "autoload-dev": {
    "classmap": ["tests/stubs"]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
